<div class="wrapper">
    <% if (allBuildingsArray != null) { %>
        <%  allBuildingsArray.forEach(building => { %>
        <a href="/buildings/<%= building._id%>">
            <div class="individual_location_wrapper"> <%= building.location  %> </div>
        </a>
        <a href="/buildings/edit/<%= building._id%>">Edit</a>
    <% }) %>
    <% } %> 
</div>