<div>
<form method="POST" action="<%= url %>?_method=PUT">
    <div>
        <label>Location</label>
        <input type="text" name="location" value="<%= building.location %>">
    </div>
    <div>
        <label>Address</label>
        <input type="text" name="address" value="<%= building.address %>">
    </div>
    <div>
        <label>Telephone Number</label>
        <input type="number" name="telephone_number" min="1" value="<%= building.telephone_number %>">
    </div>
    <div>
        <label>Tech Service</label>
        <input type="text" name="tech_service" value="<%= building.tech_service %>">
    </div>    
    <a href="/buildings">Cancel</a>
    <button type="submit">Update </button>
</form>
</div>
